      <geometry>
        <mesh filename="meshes/irb120/visual/link_2.stl" scale="0.0254 0.0254 0.0254"/>
      </geometry>
      <material name="abb_orange"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/irb120/collision/link_2.stl" scale="0.0254 0.0254 0.0254"/>
      </geometry>
      <material name="cyan"/>
    </collision>
    <inertial>
      <mass value="4.187000e+00"/>
      <inertia ixx="4.819772e-02" ixy="9.786000e-05" ixz="8.335000e-04" iyy="4.032172e-02" iyz="8.157000e-04" izz="1.476244e-02"/>
      <origin rpy="0 0 0" xyz="4.270000e-03 -2.943000e-02 1.420400e-01"/>
    </inertial>
  </link>
  <link name="${prefix}link_3">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/irb120/visual/link_3.stl" scale="0.0254 0.0254 0.0254"/>
      </geometry>
      <material name="abb_orange"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/irb120/collision/link_3.stl" scale="0.0254 0.0254 0.0254"/>
      </geometry>
      <material name="cyan"/>
    </collision>
    <inertial>
      <mass value="3.173000e+00"/>
      <inertia ixx="6.184840e-03" ixy="-1.183800e-04" ixz="1.878090e-03" iyy="1.540900e-02" iyz="5.370000e-06" izz="1.311689e-02"/>
      <origin rpy="0 0 0" xyz="4.156000e-02 4.900000e-04 5.320000e-02"/>
    </inertial>
  </link>
  <link name="${prefix}link_4">
    <visual>
      <origin xyz="-0.142 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/irb120/visual/link_4.stl" scale="0.0254 0.0254 0.0254"/>
      </geometry>
      <material name="abb_orange"/>
    </visual>
    <collision>
      <origin xyz="-0.142 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/irb120/collision/link_4.stl" scale="0.0254 0.0254 0.0254"/>
      </geometry>
      <material name="cyan"/>
    </collision>
    <inertial>
      <mass value="1.834000e+00"/>
      <inertia ixx="2.705740e-03" ixy="1.267490e-03" ixz="2.493700e-04" iyy="6.155200e-03" iyz="1.437600e-04" izz="7.059200e-03"/>
