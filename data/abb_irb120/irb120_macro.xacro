  </link>
  <link name="${prefix}tool0"/>
  <!-- end of link list -->

  <!-- joint list -->
  <joint name="${prefix}joint_1" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.163"/>
    <parent link="${prefix}base_link"/>
    <child link="${prefix}link_1"/>
    <limit effort="76.6056808" lower="${-11*pi/12}" upper="${11*pi/12}" velocity="4.36332"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="${(fvmin1+fvmax1)/2}" friction="${(fcmin1+fcmax1)/2}"/>
  </joint>
  <joint name="${prefix}joint_2" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.127"/>
    <parent link="${prefix}link_1"/>
    <child link="${prefix}link_2"/>
    <limit effort="66.795993" lower="${-11*pi/18}" upper="${11*pi/18}" velocity="4.36332"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="${(fvmin2+fvmax2)/2}" friction="${(fcmin2+fcmax2)/2}"/>
  </joint>
  <joint name="${prefix}joint_3" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.27"/>
    <parent link="${prefix}link_2"/>
    <child link="${prefix}link_3"/>
    <limit effort="32.824192" lower="${-11*pi/18}" upper="${7*pi/18}" velocity="4.36332"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="${(fvmin3+fvmax3)/2}" friction="${(fcmin3+fcmax3)/2}"/>
  </joint>
  <joint name="${prefix}joint_4" type="revolute">
    <origin rpy="0 0 0" xyz="0.142 0 0.07"/>
    <parent link="${prefix}link_3"/>
    <child link="${prefix}link_4"/>
    <limit effort="7.32805" lower="${-8*pi/9}" upper="${8*pi/9}" velocity="5.58505"/>
    <axis xyz="1 0 0"/>
    <dynamics damping="${(fvmin4+fvmax4)/2}" friction="${(fcmin4+fcmax4)/2}"/>
  </joint>
  <joint name="${prefix}joint_5" type="revolute">
    <origin rpy="0 0 0" xyz="0.16 0 0"/>
    <parent link="${prefix}link_4"/>
    <child link="${prefix}link_5"/>
    <limit effort="6.552888" lower="${-2*pi/3}" upper="${2*pi/3}" velocity="5.58505"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="${(fvmin5+fvmax5)/2}" friction="${(fcmin5+fcmax5)/2}"/>
  </joint>
  <joint name="${prefix}joint_6" type="revolute">
    <origin rpy="0 0 0" xyz="0.072 0 0"/>
    <parent link="${prefix}link_5"/>
    <child link="${prefix}link_6"/>
    <limit effort="10.03295" lower="${-20*pi/9}" upper="${20*pi/9}" velocity="7.33038"/>
    <axis xyz="1 0 0"/>
    <dynamics damping="${(fvmin6+fvmax6)/2}" friction="${(fcmin6+fcmax6)/2}"/>
  </joint>
  <joint name="${prefix}joint6-tool0" type="fixed">
    <parent link="${prefix}link_6"/>
    <child link="${prefix}tool0"/>
    <origin rpy="0 ${pi/2} 0" xyz="0 0 0"/>
  </joint>
  <!-- end of joint list -->

  <!-- ROS base_link to ABB World Coordinates transform -->
  <link name="${prefix}base" />
  <joint name="${prefix}base_link-base" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="${prefix}base_link"/>
    <child link="${prefix}base"/>
  </joint>
  </xacro:macro>
</robot>
