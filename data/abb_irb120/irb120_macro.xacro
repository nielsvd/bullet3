    </inertial>
  </link>
  <link name="${prefix}link_5">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/irb120/visual/link_5.stl" scale="0.0254 0.0254 0.0254"/>
      </geometry>
      <material name="abb_orange"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/irb120/collision/link_5.stl" scale="0.0254 0.0254 0.0254"/>
      </geometry>
      <material name="cyan"/>
    </collision>
    <inertial>
      <mass value="1.271000e+00"/>
      <inertia ixx="7.580000e-04" ixy="-2.620000e-05" ixz="-3.370000e-05" iyy="2.034000e-03" iyz="6.300000e-06" izz="1.921000e-03"/>
      <origin rpy="0 0 0" xyz="1.155000e-02 1.370000e-03 1.010000e-03"/>
    </inertial>
  </link>
  <link name="${prefix}link_6">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/irb120/visual/link_6.stl" scale="0.0254 0.0254 0.0254"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
